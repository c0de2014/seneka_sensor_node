#include "ros/ros.h"
#include "std_msgs/String.h"
#include <sstream>

int main(int argc, char **argv)
{
	ros::init(argc, argv, "imageCreator");
	ros::NodeHandle n;
	//std_msgs::String msg;
	std::stringstream ss;
	ss << "imageCreator";
	while (ros::ok()){
		ROS_INFO("%s", ss.str());
	}
	return 0;
}


